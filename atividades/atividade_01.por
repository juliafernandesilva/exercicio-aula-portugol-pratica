programa {
  funcao inicio() {
   cadeia nomeCidadeMoradia, nomeCidadeEstudo

   escreva("\nDigite a cidade onde você mora:")
   leia(nomeCidadeMoradia)

   escreva("\n\nDigite a cidade que você estuda:")
   leia(nomeCidadeEstudo)

   escreva("\nMoradia é: " + nomeCidadeMoradia + " e estudo: " + nomeCidadeEstudo)

  }
}
